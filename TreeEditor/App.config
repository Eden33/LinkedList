<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IResourceService" />
            </basicHttpBinding>
            <netTcpBinding>
                <binding name="NetTcpBinding_IResourceService" />
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://localhost:8092/ResourceService"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IResourceService"
                contract="ResourceService.IResourceService" name="NetTcpBinding_IResourceService">
                <identity>
                    <userPrincipalName value="roadrunner2\edi" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:8081/ResourceService" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IResourceService" contract="ResourceService.IResourceService"
                name="BasicHttpBinding_IResourceService" />
        </client>
    </system.serviceModel>
</configuration>